<launch>
    <node pkg="bw_object_estimation" type="object_estimation" name="object_estimation" output="screen">
        <remap from="depth/camera_info" to="/camera/image/camera_info"/>
        <remap from="depth/image_raw" to="/camera/depth/image_raw"/>
        <remap from="segmentation" to="/camera/segmentation"/>
    </node>

    <node pkg="bw_object_estimation" type="field_estimation" name="field_estimation" output="screen">
        <remap from="depth/camera_info" to="/camera/image/camera_info"/>
        <remap from="depth/image_raw" to="/camera/depth/image_raw"/>
        <remap from="segmentation" to="/camera/segmentation"/>
    </node>
</launch>
