{
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "editor.suggestSelection": "first",
    "python.languageServer": "Pylance",
    "workbench.tree.indent": 15,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "redhat.telemetry.enabled": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.formatOnSave": true,
    "python.analysis.typeCheckingMode": "basic",
    "C_Cpp.formatting": "default",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "git.openRepositoryInParentFolders": "always",
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        },
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "editor.rulers": [120],
    "python.autoComplete.extraPaths": [
        "/home/bwbots/ros_ws/devel/lib/python3/dist-packages",
        "/home/bwbots/dep_ws/install/lib/python3.9/site-packages",
        "/home/bwbots/base_ws/install/lib/python3.9/site-packages"
    ],
    "python.analysis.extraPaths": [
        "/home/bwbots/ros_ws/devel/lib/python3/dist-packages",
        "/home/bwbots/dep_ws/devel/lib/python3.9/site-packages",
        "/home/bwbots/base_ws/install/lib/python3.9/site-packages"
    ],
