<launch>
	<group ns="camera_0">
		<node pkg="bw_object_estimation" type="object_estimation" name="object_estimation"
			output="screen" respawn="true">
			<param name="queue_size" value="30" />
			<remap from="camera_info" to="rgb/camera_info" />
			<remap from="field" to="/filter/field" />
			<remap from="segmentation" to="robot/segmentation" />
		</node>
	</group>
</launch>